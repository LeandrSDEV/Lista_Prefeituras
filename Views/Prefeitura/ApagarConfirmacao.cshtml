@model PrefeituraModel

@{
    ViewData["title"] = "Excluir Prefeitura";
}

<h2 class="text-center my-4 display-3">Realmente deseja excluir (@Model.Municipio)?</h2>


<a role="button" class="btn btn-danger my-3 w-100" asp-controller="Prefeitura" asp-route-id="@Model.Id" asp-action="Excluir">Excluir</a>
<a role="button" class="btn btn-primary w-100" asp-controller="Prefeitura" asp-action="Index">Voltar</a>
